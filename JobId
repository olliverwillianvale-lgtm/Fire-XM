local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "FireTeleportGUI"
ScreenGui.Parent = game.CoreGui
ScreenGui.ResetOnSpawn = false

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 450, 0, 180)
Frame.Position = UDim2.new(0.5, -225, 0.5, -90)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 45)
Frame.BorderSizePixel = 0
Frame.Active = true
Frame.Draggable = true
Frame.Parent = ScreenGui

local UICorner = Instance.new("UICorner", Frame)
UICorner.CornerRadius = UDim.new(0, 15)

local Title = Instance.new("TextLabel")
Title.Text = "üî• Fire Teleporte"
Title.Font = Enum.Font.GothamBold
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextSize = 22
Title.BackgroundTransparency = 1
Title.Position = UDim2.new(0,0,0,15)
Title.Size = UDim2.new(1,0,0,35)
Title.Parent = Frame

local CommandBox = Instance.new("TextBox")
CommandBox.PlaceholderText = "Cole aqui o comando de teleporte"
CommandBox.Font = Enum.Font.Gotham
CommandBox.TextColor3 = Color3.fromRGB(255,255,255)
CommandBox.TextSize = 16
CommandBox.BackgroundColor3 = Color3.fromRGB(50,50,70)
CommandBox.BorderSizePixel = 0
CommandBox.Position = UDim2.new(0.05,0,0.35,0)
CommandBox.Size = UDim2.new(0.9,0,0,45)
CommandBox.ClearTextOnFocus = false
CommandBox.TextStrokeTransparency = 0.7
CommandBox.TextXAlignment = Enum.TextXAlignment.Left
CommandBox.Parent = Frame

local CommandCorner = Instance.new("UICorner", CommandBox)
CommandCorner.CornerRadius = UDim.new(0,12)

local Button = Instance.new("TextButton")
Button.Text = "Teleportar"
Button.Font = Enum.Font.GothamBold
Button.TextColor3 = Color3.fromRGB(255,255,255)
Button.TextSize = 18
Button.BackgroundColor3 = Color3.fromRGB(255, 50, 0)
Button.BorderSizePixel = 0
Button.Position = UDim2.new(0.3,0,0.75,0)
Button.Size = UDim2.new(0.4,0,0,45)
Button.Parent = Frame

local ButtonCorner = Instance.new("UICorner", Button)
ButtonCorner.CornerRadius = UDim.new(0,15)

Button.MouseEnter:Connect(function()
	Button.BackgroundColor3 = Color3.fromRGB(255,80,0)
end)
Button.MouseLeave:Connect(function()
	Button.BackgroundColor3 = Color3.fromRGB(255,50,0)
end)

Button.MouseButton1Click:Connect(function()
	local command = CommandBox.Text
	if command == "" then
		Title.Text = "‚ùå Cole o comando completo!"
		wait(1.5)
		Title.Text = "üî• Fire Teleporte"
	else
		local success, err = pcall(function()
			loadstring(command)()
		end)
		if not success then
			Title.Text = "‚ö†Ô∏è Erro ao teleportar!"
			warn(err)
			wait(2)
			Title.Text = "üî• Fire Teleporte"
		end
	end
end)
